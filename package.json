{
    "name": "save-gold-scheme",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint",
        "start": "npm run pre-start& npm run set-env-dev & ng serve --port 4203",
        "build": "ng build",
        "build-sit": "npm run set-env-sit & npm run pre-build &  ng build  --aot --base-href=/save-gold-scheme/",
        "build-uat": "npm run set-env-uat & npm run pre-build & ng build  --aot --base-href=/kiran-user/",
        "build-prod": "npm run set-env-prod & npm run pre-build  &  ng build  --aot --base-href=/save-gold-scheme/",
        "test-ci": "ng test --watch=false",
        "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
        "remove-unused-import": "node ./node_modules/tslint/bin/tslint --config tslint-imports.json --fix --project.",
        "e2e": "ng e2e",
        "pre-start": "node hooks/pre-start",
        "pre-build": "node hooks/pre-build",
        "post-build": "node hooks/post-build",
        "set-env-dev": "node hooks/set-env-dev",
        "set-env-sit": "node hooks/set-env-sit",
        "set-env-uat": "node hooks/set-env-uat",
        "set-env-prod": "node hooks/set-env-prod",
        "prepare": "husky install",
        "prepare-war-sit": "npm run build-sit && cd dist/save-gold-scheme && jar -cvf ../../save-gold-scheme.war .",
        "prepare-war-uat": "npm run build-uat && cd dist/save-gold-scheme && jar -cvf ../../save-gold-scheme.war .",
        "prepare-war-prod": "npm run build-prod && cd dist/save-gold-scheme && jar -cvf ../../save-gold-scheme.war .",
        "pre-commit": " git add save-gold-scheme.war && npx pretty-quick --staged && npm run lint --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.0.1",
        "@angular/cdk": "^14.0.1",
        "@angular/common": "^14.0.1",
