{
    "api": {
        "localUrl": "https://savegoldscheme.com/",
        "apiGateway": "https://savegoldscheme.com/back-end/public/api/",
        "contentServerGateway": "https://savegoldscheme.com/back-end/"
    },
    "localization": {
        "languages": [
            {
                "code": "ar",
                "name": "AR",
                "culture": "ar-AR"
            },
            {
                "code": "en",
                "name": "EN",
                "culture": "en-EN"
            }
        ],
        "defaultLanguage": "en"
    },
    "notifications": {
        "options": {
            "timeOut": 5000,
            "progressAnimation": "decreasing",
            "progressBar": true,
            "pauseOnHover": true,
            "positionClass": "toast-top-right",
            "toastClass": "ngx-toastr"
        }
    },
    "spinner": {
        "size": "medium",
        "bdColor": "rgba(255, 255, 255, 0.78)",
        "color": "#0075b0",
        "type": "ball-spin-clockwise",
        "fullScreen": true
    },
    "dialog": {
        "small": {
            "width": "25rem"
        },
        "medium": {
            "width": "35rem"
        },
        "large": {
            "width": "80%"
        },
        "enterAnimationDuration": "500ms",
        "exitAnimationDuration": "500ms"
    },
    "caheProvider": "sessionStorage",
    "cacheEcryption": true,
    "debugging": true,
    "verticalSlider": {
        "slidesToShow": 1,
        "slidesToScroll": 1,
        "dots": true,
        "infinite": true,
        "vertical": true,
        "autoplay": false,
        "arrows": false
    },
    "loginOtp": {
        "resendTimeout": 30000,
        "config": {
            "allowNumbersOnly": true,
            "length": 6,
            "isPasswordInput": false,
            "disableAutoFocus": true
        }
    },
